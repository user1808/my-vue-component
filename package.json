{
  "name": "@user1808/my-vue-component",
  "version": "1.2.2",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/user1808/my-vue-component.git"
  },
  "type": "module",
  "module": "./dist/my-vue-component.js",
  "types": "./dist/src/main.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "@user1808:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "create-docs": "typedoc --tsconfig ./tsconfig.docs.json",
    "dev-create-docs": "typedoc --tsconfig ./tsconfig.docs.json --watch"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.15",
    "vue": "^3.3.4"
  },
  "bundledDependencies": [
    "@headlessui/vue"
  ],
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@storybook/addon-a11y": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/blocks": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.4.0",
    "@storybook/vue3-vite": "^7.4.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^12.17.2",
    "eslint": "^8.45.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.15.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.65.1",
    "start-server-and-test": "^2.0.0",
    "storybook": "^7.4.0",
    "typedoc": "^0.25.1",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-dts": "^3.5.3",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.6"
  },
  "bundleDependencies": [
    "@headlessui/vue"
  ]
}
